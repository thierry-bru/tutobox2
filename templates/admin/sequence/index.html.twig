 <turbo-frame id="main_page">
    <h1>Sequence index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sequence in sequences %}
            <tr>
                <td>{{ sequence.id }}</td>
                <td>{{ sequence.title }}</td>
                <td>
                    <a href="{{ path('app_sequence_show', {'id': sequence.id}) }}" data-turbo-frame="main_page">show</a>
                    <a href="{{ path('app_sequence_edit', {'id': sequence.id}) }}" data-turbo-frame="main_page">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_sequence_new') }}" data-turbo-frame="main_page">Create new</a>
</turbo-frame>
