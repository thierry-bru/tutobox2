 <turbo-frame id="main_page">
    <h1>Module index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for module in modules %}
            <tr>
                <td>{{ module.id }}</td>
                <td>{{ module.title }}</td>
                <td>
                    <a href="{{ path('app_module_show', {'id': module.id}) }}" data-turbo-frame="main_page">show</a>
                    <a href="{{ path('app_module_edit', {'id': module.id}) }}" data-turbo-frame="main_page">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_module_new') }}" data-turbo-frame="main_page">Create new</a>
</turbo-frame>
